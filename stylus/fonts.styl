@font-face {
  font-family: 'fonts';
  src:  url('../assests/fonts.eot?y2wjed');
  src:  url('../assests/fonts.eot?y2wjed#iefix') format('embedded-opentype'),
    url('../assests/fonts.ttf?y2wjed') format('truetype'),
    url('../assests/fonts.woff?y2wjed') format('woff'),
    url('../assests/fonts.svg?y2wjed#fonts') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'fonts' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

